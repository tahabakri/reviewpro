{
  "name": "project-bolt",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "ts-node-dev --respawn --transpile-only server/src/index.ts",
    "build": "tsc && vite build",
