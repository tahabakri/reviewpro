    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "@gemini-ai/sdk": "^1.0.0",
    "bullmq": "^5.1.11",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "framer-motion": "^11.0.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
